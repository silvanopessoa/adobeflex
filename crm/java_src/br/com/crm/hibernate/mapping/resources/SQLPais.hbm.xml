<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping >
    <sql-query name="sql.pais">
    <return class="br.com.crm.model.entity.PaisModel" alias="model"/>
        <![CDATA[
 			select  
 				model 
 			from  
 			    tb_pais model
        ]]> 
    </sql-query >
</hibernate-mapping>

