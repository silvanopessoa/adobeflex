<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <query name="hql.cidade.all">from CidadeModel</query>
    <sql-query name="sql.cidade.all">select * from tb_cidade</sql-query>
    <sql-query name="proc.cidade.insert">{call cidades()}</sql-query>
</hibernate-mapping>
